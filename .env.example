# All app/runtime configuration should be managed through workspace_settings in DB.
DATABASE_URL="postgresql://username:password@your-neon-host/dbname?sslmode=require"

# 32-byte key encoded as 64 hex characters
# Generate with: openssl rand -hex 32
DB_ENCRYPTION_KEY="replace-with-64-char-hex"

NODE_ENV="development"

# Fall back env for CLI set up only
# Manus API
MANUS_API_KEY=""
MANUS_BASE_URL="https://api.manus.ai/v1/"
MANUS_WEBHOOK_SECRET=""
MANUS_WEBHOOK_URL="https://your-public-domain/api/manus/webhook?secret=your-secret"
MANUS_AGENT_PROFILE="manus-1.6"
MANUS_ENABLED_CONNECTOR_UUIDS=""

# Task Router LLM (optional for multi-active-task disambiguation)
ROUTER_LLM_PROVIDER="openai_compatible"
ROUTER_LLM_API_KEY=""
ROUTER_LLM_MODEL="gpt-5-mini"
ROUTER_LLM_BASE_URL="https://api.openai.com"

# WhatsApp (Baileys auth state)
WHATSAPP_AUTH_DIR="./.data/whatsapp-auth"
WHATSAPP_SESSION_NAME="default"

# Internal routes
MOCK_TOKEN=""